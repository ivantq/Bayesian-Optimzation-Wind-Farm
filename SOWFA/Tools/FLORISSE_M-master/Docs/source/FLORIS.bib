isbn = {1-56347-594-4},
journal = {ASME 2003 Wind Energy Symposium, WIND2003},
keywords = {Actuator disks,Aerodynamics,Aeroelasticity,Experiments,Finite element method,NASA,Wind power},
number = {January},
pages = {94--103},
title = {{Yaw aerodynamics analyzed with three codes in comparison with experiment}},
year = {2003}
}

@article{Aboshosha2014,
abstract = {Simulations of a horizontal axis wind turbine using direct modeling of the rotor blades are presented. The computational numerical solution was carried out in order to get a better understanding of wake development as well as the possibility to predict the power available in the wakes for the turbines located in the downstream direction. The amount of power extracted from the wind turbine and the power available in the wake were studied for different tilt angles of the blades varied from 0 to 15 degrees. However the wind turbine extracted less power by tilting the rotor blades, the power available in the wake downstream the turbine was significantly increased compared with zero tilt angle. The results show that control of the tilt angle can maximize the total power output of the wind farm. This study provides a method to control the wake actively and thus optimize the efficiency of a wind farm.},
author = {Abdelsalam, Ali M and Ramalingam, Velraj},
doi = {10.3850/978-981-07-8012-8},
isbn = {9789810780111},
journal = {Journal of Wind and Engineering},
keywords = {Power,RANS,Tilt Angles,Wakes,Wind Turbines},
number = {1},
