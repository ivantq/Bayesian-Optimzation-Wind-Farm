@article{Witha2014,
abstract = {Large-Eddy Simulations (LES) of single and multiple wakes are performed with different turbine parameterizations: A standard uniformly loaded actuator disk model (ADM), an actuator line model (ALM) and an enhanced non-uniformly loaded actuator disk model with rotation (ADM-R). The detailed near wake structure can only be captured with the complex ALM and ADM-R. The characteristics of the far wake are, however, represented with the simple ADM as well. The ADM-R results are almost identical to the ALM results. As it spends significantly less computation time, the ADM-R can be considered an adequate alternative to the ALM. The grid size is crucial only for the complex turbine models and the near wake characteristics. Here, at least 30 grid points per rotor diameter were required to adequately resolve the near wake flow. For the far wake and the ADM, much coarser grids were sufficient.},
author = {Witha, B and Steinfeld, G and Heinemann, D},
journal = {Researchgate.Net},
number = {June},
title = {{Advanced turbine parameterizations in offshore LES wake simulations}},
