@article{Bastankhah2016,
abstract = {{\textless}p{\textgreater}This work is dedicated to systematically studying and predicting the wake characteristics of a yawed wind turbine immersed in a turbulent boundary layer. To achieve this goal, wind tunnel experiments were performed to characterize the wake of a horizontal-axis wind turbine model. A high-resolution stereoscopic particle image velocimetry system was used to measure the three velocity components in the turbine wake under different yaw angles and tip-speed ratios. Moreover, power and thrust measurements were carried out to analyse the performance of the wind turbine. These detailed wind tunnel measurements were then used to perform a budget study of the continuity and Reynolds-averaged Navier–Stokes equations for the wake of a yawed turbine. This theoretical analysis revealed some notable features of the wakes of yawed turbines, such as the asymmetric distribution of the wake skew angle with respect to the wake centre. Under highly yawed conditions, the formation of a counter-rotating vortex pair in the wake cross-section as well as the vertical displacement of the wake centre were shown and analysed. Finally, this study enabled us to develop general governing equations upon which a simple and computationally inexpensive analytical model was built. The proposed model aims at predicting the wake deflection and the far-wake velocity distribution for yawed turbines. Comparisons of model predictions with the wind tunnel measurements show that this simple model can acceptably predict the velocity distribution in the far wake of a yawed turbine. Apart from the ability of the model to predict wake flows in yawed conditions, it can provide valuable physical insight on the behaviour of turbine wakes in this complex situation.{\textless}/p{\textgreater}},
author = {Bastankhah, Majid and Port{\'{e}}-Agel, Fernando},
doi = {10.1017/jfm.2016.595},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
keywords = {jets,turbulent boundary layers,turbulent flows,wakes},
pages = {506--541},
title = {{Experimental and theoretical study of wind turbine wakes in yawed conditions}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112016005954},
volume = {806},
year = {2016}
}

@article{Niayifar2015,
abstract = {In this study, a new analytical approach is presented and validated to predict wind farm power$\backslash$r production. The new model is an extension of the recently proposed by Bastankhah and Port?-Agel for$\backslash$r a single wake. It assumes a self-similar Gaussian shape of the velocity deficit and satisfies$\backslash$r conservation of mass and momentum. To estimate the velocity deficit in the wake, this model needs$\backslash$r the local wake growth rate parameter which is calculated based on the local turbulence intensity in$\backslash$r the wind farm. The interaction of the wakes is modeled by use of the velocity deficit superposition$\backslash$r principle. Finally, the power curve is used to estimate the power production from the wind turbines.$\backslash$r The wind farm model is compared to large-eddy simulation (LES) data and measurments of Horns Rev$\backslash$r wind farm for a wide range of wind directions. Reasonable agreement between the proposed analytical$\backslash$r model, LES data and measurments is obtained. This prediction is also found to be substantially$\backslash$r better than the one obtained with a commonly used wind farm wake model.},
author = {Niayifar, Amin and Port{\'{e}}-Agel, Fernando},
doi = {10.1088/1742-6596/625/1/012039},
issn = {1742-6588},
journal = {Journal of Physics: Conference Series},
pages = {012039},
title = {{A new analytical model for wind farm power prediction}},
url = {http://stacks.iop.org/1742-6596/625/i=1/a=012039?key=crossref.e168aebfa3a17acad4b99cb0176487cc},
volume = {625},
year = {2015}
}

@article{Bastankhah2014,
abstract = {A new analytical wake model is proposed and validated to predict the wind velocity distribution downwind of a wind turbine. The model is derived by applying conservation of mass and momentum and assuming a Gaussian distribution for the velocity deficit in the wake. This simple model only requires one parameter to determine the velocity distribution in the wake. The results are compared to high-resolution wind-tunnel measurements and large-eddy simulation (LES) data of miniature wind-turbine wakes, as well as LES data of real-scale wind-turbine wakes. In general, it is found that the velocity deficit in the wake predicted by the proposed analytical model is in good agreement with the experimental and LES data. The results also show that the new model predicts the power extracted by downwind wind turbines more accurately than other common analytical models, some of which are based on less accurate assumptions like considering a top-hat shape for the velocity deficit. {\textcopyright} 2014 Elsevier Ltd.},
author = {Bastankhah, Majid and Port{\'{e}}-Agel, Fernando},
doi = {10.1016/j.renene.2014.01.002},
issn = {09601481},
journal = {Renewable Energy},
keywords = {Analytical models,Gaussian model,Top-hat model,Velocity deficit,Wind-turbine wakes},
pages = {116--123},
publisher = {Elsevier Ltd},
title = {{A new analytical model for wind-turbine wakes}},
url = {http://dx.doi.org/10.1016/j.renene.2014.01.002},
volume = {70},
year = {2014}
}

